{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "MyS3ListBucketPolicy",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::awsseccookbook",
      "Condition": {
        "DateLessThan": {
          "aws:EpochTime": "1560237648"
        }
      }
    }
  ]
}